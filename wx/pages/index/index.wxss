view {
  /*会影响Shadow DOM*/
  display: flex; 
  font-size: 24px;
}
#tabs {
  /*不会影响Shadow DOM*/
  color: red; 
  font-size: 18px;
}
#fancy-tabs {
  /*会影响Shadow DOM的子项*/
  display: flex; 
  flex-direction: column;
  width: 400px;
  /*会被Shadow DOM继承*/
  color: red; 
  font-size: 20px;
  border: 1px solid black;
}