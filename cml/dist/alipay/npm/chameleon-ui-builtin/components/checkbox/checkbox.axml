<view class="cml-checkbox  cml-base cml-view cml-83e87a02">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-83e87a02" onTap="_cmlEventProxy" data-eventtap="changeCheck">
    <view class="cml-checkbox-check  cml-base cml-view cml-83e87a02" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-83e87a02">
        <image class="inner-icon  cml-base cml-image cml-83e87a02" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-83e87a02">{{label}}</text>
  </view>
</view>